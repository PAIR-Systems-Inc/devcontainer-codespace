{
  "name": "GoodMem Dev Container",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "postCreateCommand": "echo -e 'OPENAI_API_KEY=\\nADD_API_KEY=' > /workspaces/.env && curl -s https://get.goodmem.ai | bash -s -- --debug-install --no-openai-embedder-registration",
  "features": {
    "ghcr.io/devcontainers/features/docker-in-docker:2": {}
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-vscode.vscode-docker"
      ]
    }
  }
}
